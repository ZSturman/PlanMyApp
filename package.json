  "description": "React library to create Software Development Documentation",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "build:ts:esm": "tsc --project tsconfig.esm.json",
    "build:ts:cjs": "tsc --project tsconfig.cjs.json",
    "build:scss": "webpack --config webpack.scss.config.js",
    "build": "npm run clean && npm run build:ts:cjs && npm run build:ts:esm && npm run build:scss",
    "patch": "npm version patch",
    "minor": "npm version minor",
    "major": "npm version major",
    "test": "jest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "repository": {
